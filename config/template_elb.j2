object Host "{{ hostname }}" {
    import "generic-host"
    address = "{{ address }}"
    vars.elb = true
    vars.region_name = "{{ region_name }}"
    vars.dontping = true
    vars.state = "{{ state }}"
    check_command = "dummy"
    enable_active_checks = false
}
